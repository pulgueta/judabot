{
  "name": "judabot",
  "version": "0.0.0",
  "type": "module",
  "description": "Simple bot for Judamoca's Twitch channel",
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsx --env-file=.env ./src/env.ts && tsx watch --env-file=.env ./src/index.ts",
    "build": "tsup",
    "start": "node --env-file=.env ./dist/env.js && node --env-file=.env ./dist/index.mjs",
    "lint": "biome lint --write --unsafe .",
    "format": "biome format --write ."
  },
  "keywords": [],
  "author": "Andrés Rodríguez (pulgueta)",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.0.8",
    "ai": "^4.0.14",
    "zod": "^3.24.0"
  }
}
